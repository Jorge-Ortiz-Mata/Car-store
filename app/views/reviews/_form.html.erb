<%= form_with model: [@vehicle, Review.new] do |form| %>
    <div>
        <%= form.label :body %>
        <%= form.text_field :body %>
    </div>
    <div>
        <%= form.label :rating %>
        <%= form.number_field :rating %>
    </div>
    <div>
        <%= form.submit %>
    </div>
<% end %>