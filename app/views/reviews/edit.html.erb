<div>
    <h2>Edit review</h2>
    <%= form_with(model: [@vehicle, @review], local: true) do |form| %>
        <div>
            <%= form.label :body %>
            <%= form.text_field :body %>
        </div>
        <div>
            <%= form.label :rating %>
            <%= form.number_field :rating %>
        </div>
        <div>
            <%= form.submit %>
        </div>
    <% end %>
</div>